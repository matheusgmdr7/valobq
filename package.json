{
  "name": "valoren-trading-platform",
  "version": "1.0.0",
  "description": "VALOREN - Plataforma de Negociação de Ativos",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:3000": "PORT=3000 next dev --turbopack",
    "dev:3001": "PORT=3001 next dev --turbopack",
    "dev:server": "tsx src/server/MarketDataServer.ts",
    "start:server": "node --import tsx src/server/MarketDataServer.ts",
    "start:server:watch": "tsx watch src/server/MarketDataServer.ts",
    "build": "npm run build:wasm && next build",
    "build:wasm": "cd src/engine/wasm && source ~/emsdk/emsdk_env.sh 2>/dev/null || true && ./build.sh",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@supabase/supabase-js": "^2.89.0",
    "autoprefixer": "^10.4.16",
    "lightweight-charts": "^4.2.3",
    "next": "^15.5.6",
    "postcss": "^8.4.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.3.0",
    "redis": "^4.7.1",
    "tailwindcss": "^3.4.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.5.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  }
}
